# Run with screen -c <path to this file>
#
# Intended to setup kilby for the way that I like to use it.
#
caption always
screen -t dahdi-linux
register a "cd ~sruffell/dahdi-linux^M"
paste a
screen -t dahdi-tools
register a "cd ~sruffell/dahdi-tools^M"
paste a
screen -t linux-2.6
register a "cd ~sruffell/linux-2.6^M"
paste a
screen -t davinci-console 
register a "sudo picocom --baud 115200 /dev/ttyUSB1^M"
paste a
screen -t davinci 
screen -t wright-console 
register a "sudo picocom --baud 115200 /dev/ttyUSB0^M"
paste a
screen -t wright 
screen
screen -t mutt
screen -t asterisk
register a "cd ~sruffell/asterisk-trunk^M"
paste a
